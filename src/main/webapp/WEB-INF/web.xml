<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>Main</servlet-name>
		<servlet-class>ranim.servlets.Main</servlet-class>
	</servlet>
  <servlet>
		<servlet-name>Ajouter</servlet-name>
		<servlet-class>ranim.servlets.Ajouter</servlet-class>
	</servlet>
  <servlet>
		<servlet-name>Modifier</servlet-name>
		<servlet-class>ranim.servlets.Modifier</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Main</servlet-name>
		<url-pattern>/main</url-pattern>
	</servlet-mapping>
  <servlet-mapping>
    <servlet-name>Ajouter</servlet-name>
    <url-pattern>/ajouter</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Modifier</servlet-name>
    <url-pattern>/modifier</url-pattern>
  </servlet-mapping>

</web-app>
